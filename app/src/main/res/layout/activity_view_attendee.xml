<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/common_background"
    tools:context=".ViewAttendeeActivity">

    <TextView
        android:id="@+id/logo_view8"
        android:layout_width="389dp"
        android:layout_height="233dp"
        android:background="@drawable/common_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_event2"
        android:layout_width="124dp"
        android:layout_height="43dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/button_curve"
        android:text="@string/event_name"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.295" />

    <Button
        android:id="@+id/btn_attendance2"
        android:layout_width="124dp"
        android:layout_height="43dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:background="@drawable/button_curve"
        android:text="@string/attendance_name"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.295" />

    <Button
        android:id="@+id/btn_aboutus2"
        android:layout_width="124dp"
        android:layout_height="43dp"
        android:background="@drawable/button_curve"
        android:text="@string/aboutus_name"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toEndOf="@+id/btn_attendance2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.295" />

    <Button
        android:id="@+id/btn_add2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:background="@drawable/button_curve_2nd_navi"
        android:text="@string/btn_add"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.402" />

    <Button
        android:id="@+id/btn_view2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:background="@drawable/button_curve_2nd_navi"
        android:text="@string/btn_view"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.691"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.402" />

    <TextView
        android:id="@+id/recBox2"
        android:layout_width="361dp"
        android:layout_height="344dp"
        android:background="@drawable/box"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.891" />

    <TextView
        android:id="@+id/txt_EventName2"
        android:layout_width="361dp"
        android:layout_height="35dp"
        android:layout_marginTop="5dp"
        android:text="@string/event_name_example"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/recBox2" />

    <Button
        android:id="@+id/btn_option"
        android:layout_width="32dp"
        android:layout_height="36dp"
        android:layout_marginTop="88dp"
        android:drawableTop="@drawable/ic_baseline_menu_24"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.905"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/recBox2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/attendeeName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="88dp"
        android:text="@string/attendee_name"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/recBox2" />

    <TextView
        android:id="@+id/attendeeDate"
        android:layout_width="48dp"
        android:layout_height="24dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="28dp"
        android:text="@string/attendee_date"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attendeeName" />

    <TextView
        android:id="@+id/attendeeRemark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="28dp"
        android:text="@string/attendee_remark"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attendeeDate" />

    <TextView
        android:id="@+id/attendeeName_Example1"
        android:layout_width="131dp"
        android:layout_height="25dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="88dp"
        android:text="@string/attendee_name_example"
        android:textColor="#272525"
        app:layout_constraintStart_toEndOf="@+id/attendeeName"
        app:layout_constraintTop_toTopOf="@+id/recBox2" />

    <TextView
        android:id="@+id/attendeeDate_Example"
        android:layout_width="131dp"
        android:layout_height="25dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="28dp"
        android:text="@string/attendee_date_example"
        android:textColor="#272525"
        app:layout_constraintStart_toEndOf="@+id/attendeeDate"
        app:layout_constraintTop_toBottomOf="@+id/attendeeName_Example1" />

    <TextView
        android:id="@+id/attendeeRemark_Example4"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="28dp"
        android:text="@string/attendee_remark_Example"
        android:textColor="#272525"
        app:layout_constraintEnd_toEndOf="@+id/attendeeDate_Example"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/attendeeRemark"
        app:layout_constraintTop_toBottomOf="@+id/attendeeDate_Example" />

</androidx.constraintlayout.widget.ConstraintLayout>