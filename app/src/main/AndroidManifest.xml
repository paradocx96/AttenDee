<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androx.attendee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OptionEventActivity"></activity>
        <activity android:name=".DeleteEventActivity" />
        <activity android:name=".ViewEventActivity" />
        <activity android:name=".DeleteAttendeeActivity" />
        <activity android:name=".OptionAttendeeActivity" />
        <activity android:name=".ViewAttendeeActivity" />
        <activity android:name=".AttendanceHomeActivity" />
        <activity android:name=".EventHomeActivity" />
        <activity android:name=".EditAttendeeActivity" />
        <activity android:name=".AddAttendeeActivity" />
        <activity android:name=".EditEventActivity" />
        <activity android:name=".AddEventActivity" />
        <activity android:name=".HomepageActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>